 167:../mesh_common/components/connection_manager/cm_peripheral.c ****  *      addDeviceNameToAdvData
 168:../mesh_common/components/connection_manager/cm_peripheral.c ****  *
 169:../mesh_common/components/connection_manager/cm_peripheral.c ****  *  DESCRIPTION
 170:../mesh_common/components/connection_manager/cm_peripheral.c ****  *      Adds device name to advertisement or scan
 171:../mesh_common/components/connection_manager/cm_peripheral.c ****  *      response data. It follows below steps:
 172:../mesh_common/components/connection_manager/cm_peripheral.c ****  *      a. Try to add complete device name to the advertisement packet
 173:../mesh_common/components/connection_manager/cm_peripheral.c ****  *      b. Try to add complete device name to the scan response packet
 174:../mesh_common/components/connection_manager/cm_peripheral.c ****  *      c. Try to add shortened device name to the advertisement packet
 175:../mesh_common/components/connection_manager/cm_peripheral.c ****  *      d. Try to add shortened (max possible) device name to the scan
 176:../mesh_common/components/connection_manager/cm_peripheral.c ****  *         response packet
 177:../mesh_common/components/connection_manager/cm_peripheral.c ****  *
 178:../mesh_common/components/connection_manager/cm_peripheral.c ****  *  RETURNS
 179:../mesh_common/components/connection_manager/cm_peripheral.c ****  *      Nothing
 180:../mesh_common/components/connection_manager/cm_peripheral.c ****  *
 181:../mesh_common/components/connection_manager/cm_peripheral.c ****  *---------------------------------------------------------------------------*/
 182:../mesh_common/components/connection_manager/cm_peripheral.c **** static void addDeviceNameToAdvData(uint16 adv_data_len, uint16 scan_data_len)
 183:../mesh_common/components/connection_manager/cm_peripheral.c **** {
 184:../mesh_common/components/connection_manager/cm_peripheral.c ****     uint8 *p_device_name = NULL;
 185:../mesh_common/components/connection_manager/cm_peripheral.c ****     uint16 device_name_adtype_len;
 186:../mesh_common/components/connection_manager/cm_peripheral.c **** 
 187:../mesh_common/components/connection_manager/cm_peripheral.c ****     /* Read device name along with AD Type and its length */
 188:../mesh_common/components/connection_manager/cm_peripheral.c ****     p_device_name = g_cm_peripheral_data.advert_params.
 189:../mesh_common/components/connection_manager/cm_peripheral.c ****                                 advert_data.device_name.name;
 190:../mesh_common/components/connection_manager/cm_peripheral.c **** 
 191:../mesh_common/components/connection_manager/cm_peripheral.c ****     device_name_adtype_len = g_cm_peripheral_data.advert_params.
 192:../mesh_common/components/connection_manager/cm_peripheral.c ****                              advert_data.device_name.length;
 193:../mesh_common/components/connection_manager/cm_peripheral.c **** 
 194:../mesh_common/components/connection_manager/cm_peripheral.c ****     /* Add complete device name to Advertisement data */
 195:../mesh_common/components/connection_manager/cm_peripheral.c ****     p_device_name[0] = AD_TYPE_LOCAL_NAME_COMPLETE;
 196:../mesh_common/components/connection_manager/cm_peripheral.c **** 
 197:../mesh_common/components/connection_manager/cm_peripheral.c ****     /* Check if Complete Device Name can fit in remaining advertisement
 198:../mesh_common/components/connection_manager/cm_peripheral.c ****      * data space including the one byte length field which will be added
 199:../mesh_common/components/connection_manager/cm_peripheral.c ****      * by the GAP layer
 200:../mesh_common/components/connection_manager/cm_peripheral.c ****      */
 201:../mesh_common/components/connection_manager/cm_peripheral.c ****     if((device_name_adtype_len + 1) <= (MAX_ADV_DATA_LEN - adv_data_len))
 202:../mesh_common/components/connection_manager/cm_peripheral.c ****     {
 203:../mesh_common/components/connection_manager/cm_peripheral.c ****         /* Add Complete Device Name to Advertisement Data */
 204:../mesh_common/components/connection_manager/cm_peripheral.c ****         LsStoreAdvScanData(device_name_adtype_len , p_device_name,
 205:../mesh_common/components/connection_manager/cm_peripheral.c ****                       ad_src_advertise);
 206:../mesh_common/components/connection_manager/cm_peripheral.c ****     }
 207:../mesh_common/components/connection_manager/cm_peripheral.c ****     /* Check if Complete Device Name can fit in Scan response message */
 208:../mesh_common/components/connection_manager/cm_peripheral.c ****     else if((device_name_adtype_len + 1) <= (MAX_ADV_DATA_LEN - scan_data_len))
 209:../mesh_common/components/connection_manager/cm_peripheral.c ****     {
 210:../mesh_common/components/connection_manager/cm_peripheral.c ****         /* Add Complete Device Name to Scan Response Data */
 211:../mesh_common/components/connection_manager/cm_peripheral.c ****         LsStoreAdvScanData(device_name_adtype_len , p_device_name,
 212:../mesh_common/components/connection_manager/cm_peripheral.c ****                       ad_src_scan_rsp);
 213:../mesh_common/components/connection_manager/cm_peripheral.c ****     }
 214:../mesh_common/components/connection_manager/cm_peripheral.c ****     /* Check if Shortened Device Name can fit in remaining advertisement
 215:../mesh_common/components/connection_manager/cm_peripheral.c ****      * data space
 216:../mesh_common/components/connection_manager/cm_peripheral.c ****      */
 217:../mesh_common/components/connection_manager/cm_peripheral.c ****     else if((MAX_ADV_DATA_LEN - adv_data_len) >=
 218:../mesh_common/components/connection_manager/cm_peripheral.c ****             (SHORTENED_DEV_NAME_LEN + 2)) /* Added 2 for Length and AD type
 219:../mesh_common/components/connection_manager/cm_peripheral.c ****                                            * added by GAP layer
 220:../mesh_common/components/connection_manager/cm_peripheral.c ****                                            */
 221:../mesh_common/components/connection_manager/cm_peripheral.c ****     {
 222:../mesh_common/components/connection_manager/cm_peripheral.c ****         /* Add shortened device name to Advertisement data */
 223:../mesh_common/components/connection_manager/cm_peripheral.c ****         p_device_name[0] = AD_TYPE_LOCAL_NAME_SHORT;
 224:../mesh_common/components/connection_manager/cm_peripheral.c **** 
 225:../mesh_common/components/connection_manager/cm_peripheral.c ****         /* Add the short name to the advertisement data */
 226:../mesh_common/components/connection_manager/cm_peripheral.c ****         LsStoreAdvScanData(SHORTENED_DEV_NAME_LEN , p_device_name,
 227:../mesh_common/components/connection_manager/cm_peripheral.c ****                       ad_src_advertise);
 228:../mesh_common/components/connection_manager/cm_peripheral.c ****     }
 229:../mesh_common/components/connection_manager/cm_peripheral.c ****     else /* Add device name to remaining Scan reponse data space */
 230:../mesh_common/components/connection_manager/cm_peripheral.c ****     {
 231:../mesh_common/components/connection_manager/cm_peripheral.c ****         /* Add as much as can be stored in Scan Response data */
 232:../mesh_common/components/connection_manager/cm_peripheral.c ****         p_device_name[0] = AD_TYPE_LOCAL_NAME_SHORT;
 233:../mesh_common/components/connection_manager/cm_peripheral.c **** 
 234:../mesh_common/components/connection_manager/cm_peripheral.c ****         /* Add the name to the scan response */
 235:../mesh_common/components/connection_manager/cm_peripheral.c ****         LsStoreAdvScanData(MAX_ADV_DATA_LEN - scan_data_len,
 236:../mesh_common/components/connection_manager/cm_peripheral.c ****                                     p_device_name,
 237:../mesh_common/components/connection_manager/cm_peripheral.c ****                                     ad_src_scan_rsp);
 238:../mesh_common/components/connection_manager/cm_peripheral.c ****     }
 239:../mesh_common/components/connection_manager/cm_peripheral.c **** }
