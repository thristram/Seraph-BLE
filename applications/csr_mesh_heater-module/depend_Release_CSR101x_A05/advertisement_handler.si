 401:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     uint16 temp[3];
 402:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     uint16 idx = 0;
 403:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c **** 
 404:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     if (!addr) return;
 405:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c **** 
 406:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     for (idx = 0; idx < 3;)
 407:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     {
 408:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****         temp[idx] = Random16();
