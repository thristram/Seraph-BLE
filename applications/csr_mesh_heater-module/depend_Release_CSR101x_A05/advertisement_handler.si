 108:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****  *
 109:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****  * DESCRIPTION
 110:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****  *     This function is called by the application to add either advertising or scan 
 111:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****  *     response data. Each call to the function will add a single AD Structure 
 112:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****  *     (refer to Bluetooth specification Vol.3 Part C Section 11). Repeated calls 
 113:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****  *     will append new structures, to build up the data content.
 114:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****  *     The application should not include the "length" parameter within the supplied 
 115:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****  *     octet array - the GAP layer will add the length field in the appropriate 
 116:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****  *     position. The first octet of the array should be the AD Type field (see ad_type) 
 117:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****  *
 118:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****  *  PARAMETERS
 119:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****  *      param Pointer to a structure containing Advertising and Scan
 120:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****  *           timing parameters to be set.
 121:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****  *
 122:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****  *  RETURNS/MODIFIES
 123:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****  *      TRUE if data was stored successfully
 124:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****  *
 125:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****  *----------------------------------------------------------------------------*/
 126:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c **** static bool csrStoreUserAdvData(uint16 len, uint8 *data, ad_src const src)
 127:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c **** {
 128:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     uint8 offset;
 129:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     uint8 *p_data;
 130:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c **** 
 131:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     /* Move to the end of the already stored data */
 132:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     if(src == ad_src_advertise)
 133:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     {
 134:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****         p_data = ad_data;
 135:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     }
 136:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     else
 137:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     {
 138:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****         p_data = scan_rsp_data;
 139:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     }
 140:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c **** 
 141:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     if(data == NULL)
 142:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     {
 143:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****         /* Data is NULL clear the data */
 144:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****         MemSet(p_data, 0, MAX_USER_ADV_DATA_LEN);
 145:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****         return TRUE;
 146:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     }
 147:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c **** 
 148:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     offset = 0;
 149:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     while( offset < MAX_USER_ADV_DATA_LEN)
 150:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     {
 151:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****         if(p_data[offset] == 0)
 152:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****         {
 153:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****             break;
 154:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****         }
 155:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****         offset += p_data[offset] + 1;
 156:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     }
 157:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     if( (offset + len) < MAX_USER_ADV_DATA_LEN )
 158:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     {
 159:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****         p_data[offset] = (uint8)len;
 160:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****         MemCopy(p_data + offset + 1, data, len);
 161:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     }
 162:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     else
 163:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     {
 164:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****         return FALSE;
 165:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     }
 166:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     return TRUE;
 167:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c **** }
