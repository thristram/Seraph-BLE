 205:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     if((device_name_adtype_len + 1) <= (MAX_ADV_DATA_LEN - adv_data_len))
 206:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     {
 207:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****         /* Add Complete Device Name to Advertisement Data */
 208:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****         if (csrStoreUserAdvData(device_name_adtype_len , p_device_name, 
 209:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****                       ad_src_advertise) != TRUE)
 210:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****         {
 211:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****             ReportPanic(app_panic_set_advert_data);
 212:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****         }
 213:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c **** 
 214:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     }
 215:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     /* Check if Complete Device Name can fit in Scan response message */
 216:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     else if((device_name_adtype_len + 1) <= (MAX_ADV_DATA_LEN - scan_data_len)) 
 217:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     {
 218:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****         /* Add Complete Device Name to Scan Response Data */
 219:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****         if (csrStoreUserAdvData(device_name_adtype_len , p_device_name, 
 220:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****                       ad_src_scan_rsp) != TRUE)
 221:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****         {
 222:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****             ReportPanic(app_panic_set_scan_rsp_data);
 223:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****         }
 224:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c **** 
 225:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     }
 226:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     /* Check if Shortened Device Name can fit in remaining advertisement 
 227:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****      * data space 
 228:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****      */
 229:../mesh_common/mesh/handlers/advertisement/advertisement_handler.c ****     else if((MAX_ADV_DATA_LEN - adv_data_len) >=
